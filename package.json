  "name": "ericsson-udn-portal",
  "version": "2.0.0-b8",
  "description": "This is a portal for administering the Ericsson UDN.",
  "main": "dist/js/app.js",
  "author": "Idean",
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "autoprefixer-loader": "^3.2.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
