{
  "name": "ericsson-udn-portal",
  "version": "1.0.3-b1",
  "description": "This is a portal for administering the Ericsson UDN.",
  "main": "dist/js/app.js",
  "author": "Idean",
  "devDependencies": {
    "@kadira/storybook": "^2.2.0",
    "autoprefixer-loader": "^3.2.0",
    "babel": "^6.5.1",
    "babel-core": "^6.5.1",
    "babel-eslint": "^6.0.4",
    "babel-jest": "^16.0.0",
    "babel-loader": "^6.2.2",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-polyfill": "^6.7.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "copy-webpack-plugin": "^2.1.3",
    "css-loader": "^0.23.1",
    "enzyme": "^2.5.1",
    "eslint": "^2.8.0",
    "eslint-plugin-react": "^5.0.1",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "history": "^1.17.0",
    "hls.js": "^0.5.20",
    "jasmine-reporters": "^2.1.1",
    "jest-cli": "^16.0.1",
    "json-loader": "^0.5.4",
    "nightwatch": "^0.9.1",
    "node-sass": "^3.4.2",
    "papaparse": "^4.1.2",
    "preprocess": "^3.1.0",
    "preprocess-loader": "^0.2.0",
    "react-addons-test-utils": "^0.14.7",
    "react-ga": "^1.4.1",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^3.1.1",
    "require-dir": "^0.3.0",
    "sass-lint": "1.7.0",
    "sass-loader": "^3.1.2",
    "selenium-download": "^2.0.1",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.0",
    "webpack": "^1.13.2",
    "webpack-cleanup-plugin": "^0.2.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-notifier": "^1.4.1"
  },
  "dependencies": {
    "axios": "^0.9.1",
    "classnames": "^2.2.5",
    "d3": "^3.5.14",
    "dotenv": "^2.0.0",
    "downloadjs": "^1.4.2",
    "filesize": "^3.3.0",
    "html-webpack-plugin": "^2.16.0",
    "immutable": "^3.7.6",
    "immutablediff": "^0.4.2",
    "intl": "^1.2.5",
    "leaflet": "^0.7.0",
    "moment": "^2.11.2",
    "multireducer": "^3.0.0",
    "node-zendesk": "^1.1.10",
    "numeral": "^1.5.3",
    "react": "^0.14.7",
    "react-addons-css-transition-group": "^0.14.7",
    "react-bootstrap": "^0.28.2",
    "react-bootstrap-typeahead": "^0.9.4",
    "react-datepicker": "^0.21.0",
    "react-dom": "^0.14.7",
    "react-intl": "^2.1.3",
    "react-leaflet": "^0.12.3",
    "react-redux": "^4.4.0",
    "react-router": "^2.5.2",
    "react-telephone-input": "^3.5.0",
    "redux": "^3.3.1",
    "redux-actions": "^0.9.1",
    "redux-auth-wrapper": "^0.6.0",
    "redux-form": "^5.2.5",
    "redux-promise": "^0.5.1",
    "topojson": "^1.6.22",
    "uniqid": "^4.0.0",
    "validator": "^6.0.0"
  },
  "scripts": {
    "start": "node dev-server.js",
    "dist": "webpack -p --progress --config webpack/production.js --production-build --bail",
    "e2e": "./node_modules/.bin/nightwatch; exit 0;",
    "lint:js": "eslint src/ --ext .js,.jsx",
    "lint:styles": "./node_modules/.bin/sass-lint -v -q",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "storybook": "start-storybook -p 9001",
    "e2e-setup": "node selenium-download.js"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.jsx",
      "!src/**/__stories__"
    ],
    "testPathDirs": [
      "src",
      "__mocks__"
    ],
    "testPathIgnorePatterns": [
      "__tests__/coverage"
    ],
    "scriptPreprocessor": "<rootDir>/jest-preprocessor.js",
    "moduleNameMapper": {
      "^.*[.](jpg|JPG|gif|GIF|png|PNG|scss)$": "<rootDir>/SCSSStub.js"
    },
    "setupFiles": [
      "<rootDir>/jest-env-setup.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/jasmine-env-setup.js",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "automock": true,
    "unmockedModulePathPatterns": [
      "<rootDir>/src/constants",
      "<rootDir>/node_modules/axios",
      "<rootDir>/node_modules/enzyme",
      "<rootDir>/node_modules/moment",
      "<rootDir>/node_modules/numeral",
      "<rootDir>/node_modules/d3",
      "<rootDir>/node_modules/hls.js",
      "<rootDir>/node_modules/immutable",
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-bootstrap",
      "<rootDir>/node_modules/react-redux",
      "<rootDir>/node_modules/intl",
      "<rootDir>/node_modules/react-intl",
      "<rootDir>/node_modules/redux"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:VidScale/udnportal.git"
  },
  "license": "UNLICENSED",
  "private": true
}
